<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="com.otml.entities">
<class name="Order" table ="order_info">
<id name="orderNo" column="order_no"> 
<generator class="increment"></generator>
</id>
<property name="customerName" column="customer_nm"/>
<property name="mobileNo" column="mobile_no"/>
<property name="emailId" column="email_id"/>
<property name="amount"/>
<property name="orderDate" column="order_dt"/>
</class>
</hibernate-mapping>